{
  "manifest_version": 3,
  "name": "OpenFav",
  "version": "0.1",
  "description": "Save and categorize your favorite websites",
  "permissions": [
    "activeTab",
    "cookies",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}